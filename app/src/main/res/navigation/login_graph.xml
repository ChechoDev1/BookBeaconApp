<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.projectbookbeaconapp.MainFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_firstFragment_to_startFragment"
            app:destination="@id/startFragment" />
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_register_1" >
        <action
            android:id="@+id/action_secondFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
        <action
            android:id="@+id/action_secondFragment_to_firstFragment"
            app:destination="@id/firstFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFragment2"
        android:label="fragment_third"
        tools:layout="@layout/fragment_register_2" >
        <action
            android:id="@+id/action_thirdFragment_to_secondFragment"
            app:destination="@id/secondFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            />
        <action
            android:id="@+id/action_thirdFragment_to_fourthFragment"
            app:destination="@id/fourthFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFragment3"
        android:label="RegisterFragment3"
        tools:layout="@layout/fragment_register_3" >
        <action
            android:id="@+id/action_fourthFragment_to_thirdFragment"
            app:destination="@id/thirdFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_fourthFragment_to_fifthFragment"
            app:destination="@id/fifthFragment" />
    </fragment>
    <fragment
        android:id="@+id/fifthFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFragment4"
        android:label="RegisterFragment4"
        tools:layout="@layout/fragment_register_4" >
        <action
            android:id="@+id/action_fifthFragment_to_fourthFragment"
            app:destination="@id/fourthFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_fifthFragment_to_sixthFragment"
            app:destination="@id/sixthFragment" />
    </fragment>

    <fragment
        android:id="@+id/sixthFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFragment5"
        android:label="RegisterFragment5"
        tools:layout="@layout/fragment_register_5" >

        <action
            android:id="@+id/action_sixthFragment_to_fifthFragment"
            app:destination="@id/fifthFragment" />
        <action
            android:id="@+id/action_sixthFragment_to_seventhFragment"
            app:destination="@id/seventhFragment" />
        <action
            android:id="@+id/action_sixthFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>

    <fragment
        android:id="@+id/seventhFragment"
        android:name="com.example.projectbookbeaconapp.RegisterFinal"
        android:label="RegisterFragment6"
        tools:layout="@layout/fragment_register_6" >
        <argument
            android:name="correo"
            app:argType="string" />
        <argument android:name="contraseÃ±a" />
        <action
            android:id="@+id/action_seventhFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.projectbookbeaconapp.StartFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_start" />
</navigation>